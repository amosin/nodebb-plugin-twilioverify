{
  "name": "nodebb-plugin-twilioverify",
  "version": "0.0.1",
  "description": "Phone verification for NodeBB using Twilio",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/amosin/nodebb-plugin-twilioverify"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "sms",
    "twilio"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "amosin",
    "email": "andre@mosin.com.br"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amosin/nodebb-plugin-twilioverify/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-angular": "^7.1.2",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "husky": "^2.4.0",
    "twilio": "^3.49.1",
    "lint-staged": "^10.0.9"
  }
}
