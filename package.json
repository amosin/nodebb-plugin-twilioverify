{
  "name": "nodebb-plugin-twilioverify",
  "version": "0.0.1",
  "description": "Phone verification for NodeBB using Twilio",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/amosin/nodebb-plugin-twilioverify"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "sms",
    "twilio"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "amosin",
    "email": "andre@mosin.com.br"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/amosin/nodebb-plugin-twilioverify/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.13.0"
  },
  "dependencies": {
    "twilio": "^3.51.1"
  }
}
